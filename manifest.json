{
  "name": "Gara-CookieClearer",
  "manifest_version": 3,
  "description":"Change web page background",
  "version": "1.0",
  "icons":{
    "16":"./icons/Logo16.png",
    "48":"./icons/Logo48.png",
    "128":"./icons/Logo128.png"
  },
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "CookieClearer.html"
  }
}